{
  "name": "bisom_admin",
  "version": "2019.08.27.1",
  "scripts": {
    "build": "vite build --mode=production",
    "build_prod": "NODE_ENV=production vite build --mode=production",
    "build_stg": "NODE_ENV=staging vite build --mode=staging",
    "coverage": "vitest run --coverage",
    "dev": "vite --host",
    "heroku-postbuild": "vite build --mode=${NODE_ENV}",
    "heroku_stg": "vite build --mode=heroku_stg",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview --port 5050",
    "serve": "vite preview",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test": "vitest --environment jsdom",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vuetify": "^3.1.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "webpack-plugin-vuetify": "^2.0.1"
  }
}
